@import backend_colors
@import main
@import topics_timeline

@keyframes rotate3dl
	0%
		prefixs(transform, rotateY(90deg))
	10%
		prefixs(transform, rotateY(0deg))
	90%
		prefixs(transform, rotateY(0deg))
	100%
		prefixs(transform, rotateY(-90deg))

@keyframes rotate3dr
	0%
		prefixs(transform, rotateY(-90deg))
	10%
		prefixs(transform, rotateY(0deg))
	90%
		prefixs(transform, rotateY(0deg))
	100%
		prefixs(transform, rotateY(90deg))


#curso-logo
	#left
		background-image url('../img/pylogo.png')
		prefixs(animation, rotate3dl 4s normal infinite)
	#right
		background-image url('../img/dj-logo.png')
		prefixs(animation, rotate3dr 4s normal infinite)

